/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "paths": {
      "@app/*":["app/*"],
      "@env/*":["app/environments/*"],
      "@utils/*":["app/utils/*"],
      "@models/*":["app/models/*"],
      "@interfaces/*":["app/utils/interfaces*"],
      "@components/*":["app/components/*"],
      "@pages/*":["app/views/pages*"],
      "@templates/*":["app/views/templates*"],
    },
    "types": [
      "@angular/localize"
    ]
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
